<%= osu.block_to_partial 'shared/modal',
                         html_id: 'admin_console_dialog',
                         width: '900px',
                         height: '500px' do %>

  <div class="modal-header" style="border-bottom: none">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
    <ul class="nav nav-tabs tight-bottom">
      <li class="active"><a href="#users-tab" data-toggle="tab">Users</a></li>
      <li class="active"><a href="#misc-tab" data-toggle="tab">Misc</a></li>

    </ul>
  </div>

  <div class="modal-body">

    <div id="dialog-local-alerts"></div>

    <div class="tab-content">
      <div class="tab-pane active" id="users-tab">
        <%= render :partial => 'openstax/accounts/shared/accounts/index',
                   :locals => { :search_action_path => admin_accounts_path,
                                :remote => true } %>
      </div>

      <div class="tab-pane" id="misc-tab">
        <%= render 'misc' %>
      </div>
    </div>
    
  </div>

<% end %>
